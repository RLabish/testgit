#login {
    background-color: white;
        border: 1px solid #3B81B5;
    margin: 100px auto;
    padding: 15px 15px 5px 15px;
    width: 330px;
}

#login #logo {
    height: 0;
    left: -17px;
    margin: 0;
    padding: 0;
    position: relative;
    top: -45px;
    width: 0;
}

#login h1 {
    color: #3B60AF;
        font-size: 16px;
    font-weight: bold;
    margin-bottom: 0.75em;
}


#login .row.username {
    float: left;
    margin-top: 0;
}

#login .row.password {
    float: right;
    margin-left: 10px;
    margin-top: 0;
}

#login .row.buttons {
    .clear: both;
        float: none;
}

#login #LoginForm_username,
#login #LoginForm_password {
max-width: 150px;
}
	

div.form .rememberMe input {
    float: left;
    margin: 0 5px 0 0;
}

div.form .rememberMe label {
    display: block;
