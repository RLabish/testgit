/*************   filter *************/

.filter{
	font-size: 11pt;
    background-color: #E1ECEE;
    height: 34px;
    margin-top: 0;
}

div.filter .row {
/*  color: red; */
  float: left;
  clear: none;  
}

.filter .row  label,
.filter .row input,
.filter .row select {
 font-size: 11px;
}

.filter .row  label{
  width: auto;
  margin: 5px 5px 0px 20px;
  float: left;
}

.filter .row input,
.filter .row select {
  margin: 2px 0 0 0;
  padding: 1px;
  width: 100px;
  min-width: 70px;
  margin-left: 0px;
}

.filter .row input.date   {
  min-width: 0px;
  width: 70px;
}

.filter .buttons{
  padding-left: 30px;
}

.filter #submit {
    font-size: 11px;
    font-weight: bold;
    margin-top: 1px;
    min-width: 0;
    padding: 2px 10px;
}

